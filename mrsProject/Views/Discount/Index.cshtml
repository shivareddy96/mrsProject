@model IEnumerable<mrsProject.Models.Discount>

@{
    ViewData["Title"] = "Index";
}

<h2>Discount</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CouponCode)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.DiscountActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CouponCode)
                </td>
             
                <td>
                    @Html.DisplayFor(modelItem => item.DiscountActive)
                </td>
                
                
            </tr>
        }
    </tbody>
</table>